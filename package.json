{
  "name": "thy-weaver",
  "type": "module",
  "version": "0.0.0",
  "template-version": "0.0.0",
  "description": "A bun-based starter template for a modern Twine development workflow",
  "scripts": {
    "serve": "cd ./dist && bun run browser-sync start --server --watch --no-open --no-inject-changes --no-ghost-mode --no-ui --reload-delay '1000'",
    "dev-watch": "NODE_ENV=development bun run --no-clear-screen ./build.ts & bun run serve",
    "dev-watch:no-server": "NODE_ENV=development bun run --no-clear-screen ./build.ts",
    "prod": "NODE_ENV=production bun run ./build.ts"
  },
  "template-author": "Iceman8911",
  "license": "MIT",
  "template-license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.2.2",
    "@types/clean-css": "^4.2.11",
    "@types/fs-extra": "^11.0.4",
    "@types/twine-sugarcube": "^2.37.2",
    "browser-sync": "^3.0.3",
    "@parcel/watcher": "^2.5.1",
    "postcss": "^8.5.2",
    "tweenode": "^0.2.9",
    "typescript": "^5.7.3",
    "autoprefixer": "^10.4.20",
    "clean-css": "^5.3.3",
    "fs-extra": "^11.3.0",
    "sass-embedded": "^1.85.0"
  },
  "dependencies": {}
}
